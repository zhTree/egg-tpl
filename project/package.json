{
  "name": "-name",
  "version": "-vs",
  "description": "-des",
  "author": "-author",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.0",
    "egg-validate": "^2.0.2",
    "mysql2": "^1.7.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-cli",
    "restart:qa": "git pull && npm stop && npm run qa",
    "qa": "egg-scripts start --daemon --title=egg-cli --env=qa --port=8888",
    "prd": "npm stop && npm run prod",
    "prod": "egg-scripts start --daemon --title=egg-cli --env=prod --port=8888",
    "stop": "egg-scripts stop --title=egg-cli",
    "dev": "egg-bin dev --port=8888",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT"
}
